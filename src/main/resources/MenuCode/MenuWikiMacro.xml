    <property>
      <code>#template('colorThemeInit.vm')
.menu-type-horizontal {  
  height: 36px;
  background-color: $theme.menuBackgroundColor;
}

.menu-type-horizontal ul {
  list-style-type: none;
  margin: auto;
}

.menu-type-vertical ul {
  list-style-type: none;
  margin: auto;
}

.menu-type-horizontal ul li {
  display: block; 
  float: left;
  background-color: $theme.menuBackgroundColor;
  position: relative;
}

.menu-type-horizontal ul li ul li {
  float: none;
}

.menu-type-horizontal a {
  display: block;
  padding: 10px 15px;  
  font-size: 0.9em;
}

.menu-type-vertical a {
  display: block;
  padding: 10px 15px;  
  font-size: 0.9em;
}


.menu-type-horizontal a {
  color: $theme.menuLinkColor;
}

.menu-type-vertical ul li ul {
  position: relative;
}

.menu-type-horizontal ul li ul {
  display: block;
  z-index: 99999;
  top: 0;
  left: 0;
}

.menu-type-horizontal ul li ul li {
  display: block; 
  top: 0;
  left: 0;
}

.menu-type-vertical ul li ul li {
  display: block;  
}

.menu-type-vertical {
  position: relative;
}

.menu-type-vertical ul li ul {
  background-color: $theme.menuBackgroundColor;
}

.menu-type-vertical ul li ul li {
  background-color: $theme.menuBackgroundColor;
}

.menu-type-vertical a {
  color: $theme.menuLinkColor;
}

.menu-type-vertical ul li ul li a {
  color: $theme.menuLinkColor;
}

.menu-type-vertical ul li {
  display: block;
  background-color: $theme.menuBackgroundColor;
  margin-bottom: 1px;
}

.menu-type-vertical ul li ul {
  margin:0;
}</code>
